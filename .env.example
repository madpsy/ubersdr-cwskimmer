# CW Skimmer Docker Configuration
# Copy this file to .env and customize the values

# Station Configuration
CALLSIGN=MM3NDH
NAME=Nathan
QTH=Dalgety Bay
SQUARE=IO86ha

# UberSDR Configuration
UBERSDR_HOST=ka9q_ubersdr
UBERSDR_PORT=8080

# Service Control
# Set to false to keep container running without starting CW Skimmer services
# Restart trigger watcher remains active for remote enable/disable
CWSKIMM_ENABLED=true

# Band Selection for 192 kHz Mode
# Set to true/false to enable/disable each band
# These control which bands CW Skimmer will monitor
BAND_160M=false
BAND_80M=true
BAND_60M=false
BAND_40M=true
BAND_30M=true
BAND_20M=true
BAND_17M=true
BAND_15M=true
BAND_12M=true
BAND_10M=true
